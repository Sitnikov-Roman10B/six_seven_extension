{
  "manifest_version": 3,
  "name": "Six Seven(67)",
  "version": "6.7",
  "description": "Six seven",
  "icons": {  "256":"icons/icon256.png",
              "128":"icons/icon128.png",
              "67":"icons/icon67.png",
              "64":"icons/icon64.png",
              "32":"icons/icon32.png",
              "16":"icons/icon16.png"
            },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
        "default_popup":"popup/popup.html"
    },
    "permissions": ["scripting", "activeTab", "tabs"],
    "host_permissions": ["https://*/*"]
}